<script>
    console.log(<%- JSON.stringify(formularios) %>)
    var formularios = <%- JSON.stringify(formularios) %>
        console.log(formularios)
    console.log('holaaaa')


    var idFormulario = formularios.map(
        function (formulario) {
            return formulario.fech_for;
        }
    )

    var valoresFormulario = formularios.map(
        function (formulario) {
            return formulario.id_for;

        }
    )

    var datos = {
        labels : idFormulario,
        datasets : [{
            label : "datos 1",
            backgroundColor : "rgba(220,220,220,0.5)",
            data : valoresFormulario
        }
        ]
    };

    console.log(datos)

    var canvas = document.getElementById('chart2').getContext('2d');
    window.bar = new Chart(canvas, {
        type : "bar",
        data : datos,
        options : {
            elements : {
                rectangle : {
                    borderWidth : 1,
                    borderColor : "rgb(0,255,0)",
                    borderSkipped : 'bottom'
                }
            },
            responsive : true,
            title : {
                display : true,
                text : "Prueba de grafico de barras"
            }
        }
    });

</script>

